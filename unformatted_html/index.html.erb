<html>
 <head>
 </head>
 <body>
  <div class="w-full">
   &lt;% if notice.present? %&gt;
   <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice">
    &lt;%= notice %&gt;
   </p>
   &lt;% end %&gt;
   <div class="flex justify-between items-center pb-8">
    <h1 class="fmx-auto text-lg font-bold text-4xl">
     Products
    </h1>
    &lt;%= link_to "New product", new_product_path, class: "rounded-lg py-3 px-5 bg-blue-600 text-white block font-medium" %&gt;
   </div>
   <table class="mx-auto" id="products">
    <tfoot>
     <tr>
      <td colspan="3">
       <div class="mt-8">
        &lt;%= link_to 'New product',
      new_product_path,
      class: "inline rounded-lg py-3 px-5 bg-green-600
      text-white block font-medium" %&gt;
       </div>
      </td>
     </tr>
    </tfoot>
    <tbody>
     <tr>
      <td>
      </td>
     </tr>
    </tbody>
   </table>
   <div class="min-w-full" id="products">
    &lt;%= render @products %&gt;
   </div>
  </div>
 </body>
</html>
